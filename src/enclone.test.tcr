#!/bin/csh

set rootdir = `dirname $0`

enclone \
    TCR=`cat $rootdir/enclone.testdata.tcr | tail -n +3 | grep -v '#' | \
        tr -d ' ' | head --bytes=-1 | tr '\n' ';' | tr -s ';'` \
    ANN COMP BC FAIL_ONLY=true PLAIN NSILENT NO_REUSE PRINT_FAILED_JOINS \
    PRE=/mnt/assembly/vdj/current14 $*
