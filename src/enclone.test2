#!/bin/csh

set rootdir = `dirname $0`

enclone \
    BCR=`cat $rootdir/testdata.public.bcr.human | grep -v '#' | tr -d ' ' | grep -v '^$' | head --bytes=-1 | \
        tr '\n' ';' | tr -s ';'` \
    ANN COMP BC FAIL_ONLY=true PLAIN NSILENT NO_REUSE PRINT_FAILED_JOINS \
    MIX_DONORS PRE=/mnt/assembly/vdj/current14 $*
