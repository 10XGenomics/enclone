#!/bin/bash

rootdir=$(dirname $0)

enclone \
    BCR=`cat $rootdir/enclone.testdata | tail -n +3 | grep -v '#' | \
        tr -d ' ' | head --bytes=-1 | tr '\n' ';' | tr -s ';'` \
    ANN COMP BC FAIL_ONLY=true PLAIN NSILENT NO_REUSE PRINT_FAILED_JOINS \
    PRE=/mnt/assembly/vdj/current13 NDONOR REQUIRED_FPS=15 $*
