name:     near
inputs:   lvar_vdj
limits:
class:    lvar
level:    exact
val:      nonnegative_integer
doc:      TBD
brief:    Hamming distance to nearest neighbor 
page:     enclone help lvars
avail:    public
notes:
code:     let near;
          let mut dist = 1_000_000;
          for i2 in 0..varmat.len() {
              if i2 == u || fp[i2] != fp[u] {
                  continue;
              }
              let mut d = 0;
              for c in fp[u].iter() {
                  for j in 0..varmat[u][*c].len() {
                      if varmat[u][*c][j] != varmat[i2][*c][j] {
                          d += 1;
                      }
                  }
              }
              dist = min(dist, d);
          }
          if dist == 1_000_000 {
              near = "".to_string()
          } else {
              near = format!("{}", dist)
          }
          exact: near
