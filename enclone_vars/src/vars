                              dna.append(&mut z);
                          }
                      }
                      if x.seq_del_amino[p] != b'-' {
                          dna.push(x.seq_del_amino[p]);
                      }
                  }
                  test_internal_error_seq(&x.seq, &dna, &x.cdr3_aa)?;
                  y = stringme(&aa_seq(&dna, 0));
              }
          }
          exact: y
