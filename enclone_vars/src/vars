name:     udiff
inputs:   cvar_vdj
limits:
class:    cvar
level:    exact
val:      string
doc:      TBD
brief:    differences of 5'-UTR region with universal reference
page:     enclone help cvars
avail:    public
notes:
code:     let ulen = ex.share[mid].v_start;
          let uid = ex.share[mid].u_ref_id;
          let mut udiff = String::new();
          let mut ndiffs = 0;
          if uid.is_some() {
              let r = &refdata.refs[uid.unwrap()];
              let mut extra = 0;
              if ulen > r.len() {
                  extra = ulen - r.len();
              }
              for i in 0..ulen {
                  let mut rpos = i;
                  if ulen < r.len() {
                      rpos += r.len() - ulen;
                  } else {
                      if i + r.len() < ulen {
                          continue;
                      }
                      rpos -= ulen - r.len();
                  }
                  let tb = ex.share[mid].full_seq[i];
                  let rb = r.to_ascii_vec()[rpos];
                  if tb != rb {
                      ndiffs += 1;
                      if ndiffs <= 5 {
                          udiff += &format!("{}{}", rpos, tb as char);
                      }
                  }
              }
              if ndiffs > 5 {
                  udiff += "...";
              }
              if extra > 0 {
                  udiff += &format!("+{}", extra);
              }
          } else if ulen > 0 {
              udiff = format!("+{}", ulen);
          }
          exact: udiff
